{
    "name": "jaravel/laravel-filesystem-cos",
    "description": "A Cos storage filesystem for Laravel.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "qcloud/cos-sdk-v5": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Jaravel\\Flysystem\\Cos\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaravel\\Flysystem\\Cos\\CosStorageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
